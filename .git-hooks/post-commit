#!/bin/sh
# Auto-push despuÃ©s de commit
# Este hook se ejecuta automÃ¡ticamente despuÃ©s de cada commit
# Configurado para usar UTF-8

export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8

